{
  "name": "ibis",
  "version": "1.0.0",
  "description": "Bird API",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --recursive",
    "start": "node index.js",
    "create-db": "createdb ibis || true",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "setup": "npm run create-db && npm run migrate && npm run seed",
    "build": "docker build -f Dockerfile -t ibis:latest ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/birdrapp/ibis.git"
  },
  "keywords": [
    "birdr"
  ],
  "author": "Matthew Williams",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/birdrapp/ibis/issues"
  },
  "homepage": "https://github.com/birdrapp/ibis#readme",
  "dependencies": {
    "body-parser": "^1.17.0",
    "boom": "^4.2.0",
    "change-case": "^3.0.1",
    "express": "^4.15.0",
    "joi": "^10.2.2",
    "knex": "^0.12.7",
    "lodash": "^4.17.4",
    "pg": "^6.1.2",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "mocha": "^3.2.0",
    "sinon": "^1.17.7",
    "supertest": "^3.0.0"
  }
}
